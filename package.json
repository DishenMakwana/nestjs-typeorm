{
  "name": "nest-typeorm",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,db}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "changelog": "auto-changelog --template changelog-template.hbs -p -u --commit-limit false",
    "changelog-debug": "auto-changelog --template changelog-template.hbs -p --template json --output changelog-data.json",
    "prepare": "husky install",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js -d src/database/data-source.ts",
    "typeorm:prod": "./node_modules/.bin/typeorm -d dist/src/database/data-source.js",
    "typeorm:seed": "node ./node_modules/typeorm-seeding/dist/cli.js",
    "migration:generate": "yarn run typeorm migration:generate",
    "migration:create": "yarn run typeorm migration:create",
    "migration:run": "yarn run typeorm migration:run",
    "migration:revert": "yarn run typeorm migration:revert",
    "migration:show": "yarn run typeorm migration:show",
    "schema:sync": "yarn run typeorm schema:sync",
    "schema:drop": "yarn run typeorm schema:drop",
    "schema:log": "yarn run typeorm schema:log",
    "seed:config": "yarn run typeorm:seed config",
    "seed:run": "yarn run typeorm:seed seed",
    "custom:seed:run": "ts-node ./src/database/seeds/run-seed.ts",
    "migration:run:prod": "yarn run typeorm:prod migration:run",
    "migration:revert:prod": "yarn run typeorm:prod migration:revert",
    "migration:show:prod": "yarn run typeorm:prod migration:show",
    "schema:sync:prod": "yarn run typeorm:prod schema:sync",
    "schema:drop:prod": "yarn run typeorm:prod schema:drop",
    "schema:log:prod": "yarn run typeorm:prod schema:log",
    "seed:run:prod": "yarn run typeorm:seed seed"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "aws-sdk": "^2.1284.0",
    "bcryptjs": "^2.4.3",
    "camelize": "^1.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express-basic-auth": "^1.2.1",
    "handlebars": "^4.7.7",
    "hbs": "^4.2.0",
    "helmet": "^6.0.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nestjs-i18n": "^10.2.3",
    "nodemailer": "^6.8.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.8.0",
    "pusher": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.11",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/faker": "^6.6.9",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "auto-changelog": "^2.4.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.2",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "auto-changelog": {
    "commitLimit": false,
    "unreleased": true,
    "issueUrl": "https://github.com/dishenmakwana/nest-typeorm/templates/nest-template/-/issues",
    "replaceText": {
      "[Ee][Nn][Hh]:": "",
      "[Ff][Ii][Xx]:": "",
      "[Cc][Hh][Gg]:": ""
    },
    "includeBranch": [
      "development",
      "main"
    ]
  },
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19"
  }
}
